@model IEnumerable<MMS.Data.Models.Member>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>MMS </h1><h3>- Member Management Solution</h3>
    <div>
        There are @Model.Count() Members in the db.
    </div>
    <hr />
    <ol>
        @foreach(var member in Model)
        {
            <li>@member.FirstName @member.LastName</li>
        }
    </ol>
    
</div>

